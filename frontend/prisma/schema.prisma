    tot_inorg_nitro      Float?
    /// @zod.custom.use(z.coerce.number())
    tot_nitro            Float?
    /// @zod.custom.use(z.coerce.number())
    tot_part_carb        Float?
    /// @zod.custom.use(z.coerce.number())
    tot_org_carb         Float?
    /// @zod.custom.use(z.coerce.number())
    tot_org_c_meth       Float?
    /// @zod.custom.use(z.coerce.number())
    tot_nitro_content    Float?
